#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/web/小工具/Tracker/node_modules/.store/prettier@3.2.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/web/小工具/Tracker/node_modules/.store/prettier@3.2.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/prettier@3.2.2/node_modules/prettier/bin/prettier.cjs" "$@"
else
  exec node  "$basedir/../.store/prettier@3.2.2/node_modules/prettier/bin/prettier.cjs" "$@"
fi
